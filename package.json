{
  "name": "RuukkiApp",
  "version": "1.0.0",
  "description": "Ruukki Web",
  "main": "index.js",
  "scripts": {
    "dev": "webpack-dev-server --hot --mode development",
    "start": "node ./node_modules/webpack-dev-server/bin/webpack-dev-server.js --mode development --open",
    "serve": "live-server --port=8080 dist/",
    "clean": "rm -rfv ./dist",
    "watch": "node node_modules/webpack/bin/webpack.js --mode development --watch",
    "build-dev": "node node_modules/webpack/bin/webpack.js --mode development",
    "build": "node node_modules/webpack/bin/webpack.js --mode production",
    "pack-dev": "rm -rfv ./dist && node node_modules/webpack/bin/webpack.js --mode development && rm -rfv bonita/ && mkdir ./bonita && cp ./page.properties ./bonita && mkdir -p bonita/resources && cp dist/* bonita/resources/ -R && cd ./bonita && zip -r contacts-page.zip ./*",
    "pack": "rm -rfv ./dist && node node_modules/webpack/bin/webpack.js --mode production && rm -rfv bonita/ && mkdir ./bonita && cp ./page.properties ./bonita && mkdir -p bonita/resources && cp dist/* bonita/resources/ -R && cd ./bonita && zip -r contacts-page.zip ./*"
  },
  "author": "Tung Nguyen",
  "license": "ISC",
  "dependencies": {
    "axios": "^0.18.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "chartist": "^0.11.0",
    "react": "^16.2.0",
    "react-chartist": "^0.13.1",
    "react-dom": "^16.2.0",
    "react-google-maps": "^9.4.5",
    "react-pure-render": "^1.0.2",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.2.2",
    "recompose": "^0.26.0",
    "redux": "^3.7.2",
    "redux-form": "^7.3.0",
    "redux-promise": "^0.5.3",
    "webpack": "^4.2.0"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.4",
    "babel-preset-env": "^1.6.1",
    "copy-webpack-plugin": "^4.5.1",
    "css-loader": "^0.28.11",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "file-loader": "^1.1.11",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.1.0",
    "style-loader": "^0.20.3",
    "webpack-cli": "^2.0.13",
    "webpack-combine-loaders": "^2.0.3",
    "webpack-dev-server": "^3.1.1"
  }
}
